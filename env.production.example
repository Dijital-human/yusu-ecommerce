# ===========================================
# OAUTH PROVIDERS / OAUTH PROVIDER-LƏR
# ===========================================
# Google OAuth Production Keys / Google OAuth Production Açarları
GOOGLE_CLIENT_ID="your-google-production-client-id"
GOOGLE_CLIENT_SECRET="your-google-production-client-secret"

# Facebook OAuth Production Keys / Facebook OAuth Production Açarları
FACEBOOK_CLIENT_ID="your-facebook-production-client-id"
FACEBOOK_CLIENT_SECRET="your-facebook-production-client-secret"

# Apple OAuth Production Keys / Apple OAuth Production Açarları
APPLE_CLIENT_ID="your-apple-production-client-id"
APPLE_CLIENT_SECRET="your-apple-production-client-secret"

# ===========================================
# PAYMENT GATEWAY / ÖDƏNİŞ GATEWAY-İ
# ===========================================
# Stripe Live Keys / Stripe Live Açarları
STRIPE_PUBLISHABLE_KEY="pk_live_your_stripe_publishable_key"
STRIPE_SECRET_KEY="sk_live_your_stripe_secret_key"
STRIPE_WEBHOOK_SECRET="whsec_your_stripe_webhook_secret"

# ===========================================
# EXTERNAL APIS / XARİCİ API-LƏR
# ===========================================
# Google Maps API Production Key / Google Maps API Production Açarı
NEXT_PUBLIC_GOOGLE_MAPS_API_KEY="your-google-maps-production-api-key"

# ===========================================
# EMAIL CONFIGURATION / EMAIL KONFİQURASİYASI
# ===========================================
# SMTP Configuration for production emails / Production email-lər üçün SMTP konfiqurasiyası
EMAIL_SERVER_HOST="smtp.gmail.com"
EMAIL_SERVER_PORT="587"
EMAIL_SERVER_USER="noreply@yusu.com"
EMAIL_SERVER_PASSWORD="your-production-app-password"
EMAIL_FROM="noreply@yusu.com"

# ===========================================
# REDIS CONFIGURATION / REDIS KONFİQURASİYASI
# ===========================================
# Redis URL for production caching / Production keşləmə üçün Redis URL-i
REDIS_URL="redis://your-redis-host:6379"

# ===========================================
# APPLICATION SETTINGS / TƏTBİQ TƏNZİMLƏRİ
# ===========================================
# Node environment / Node mühiti
NODE_ENV="production"

# Application port / Tətbiq portu
PORT="3000"

# ===========================================
# SECURITY SETTINGS / TƏHLÜKƏSİZLİK TƏNZİMLƏRİ
# ===========================================
# CORS origins / CORS mənbələri
CORS_ORIGIN="https://yusu.com,https://admin.yusu.com,https://seller.yusu.com,https://courier.yusu.com"

# Rate limiting / Rate limiting
RATE_LIMIT_MAX="1000"
RATE_LIMIT_WINDOW_MS="900000"

# Security headers / Təhlükəsizlik başlıqları
SECURITY_HEADERS="true"
CSP_POLICY="default-src 'self'"

# ===========================================
# LOGGING CONFIGURATION / LOGGING KONFİQURASİYASI
# ===========================================
# Log level / Log səviyyəsi
LOG_LEVEL="info"

# Log file location / Log fayl yeri
LOG_FILE="/var/log/yusu/ecommerce.log"

# ===========================================
# FILE UPLOAD CONFIGURATION / FAYL YÜKLƏMƏ KONFİQURASİYASI
# ===========================================
# Upload directory / Yükləmə qovluğu
UPLOAD_DIR="/var/uploads/yusu"

# Max file size (in bytes) / Maksimum fayl ölçüsü (bayt)
MAX_FILE_SIZE="10485760"

# Allowed file types / İcazə verilən fayl növləri
ALLOWED_FILE_TYPES="image/jpeg,image/png,image/gif,image/webp"

# ===========================================
# CDN CONFIGURATION / CDN KONFİQURASİYASI
# ===========================================
# CDN URL for static assets / Statik aktivlər üçün CDN URL-i
CDN_URL="https://cdn.yusu.com"

# ===========================================
# MONITORING CONFIGURATION / MONİTORİNG KONFİQURASİYASI
# ===========================================
# Sentry DSN for error tracking / Xəta izləmə üçün Sentry DSN
SENTRY_DSN="your-sentry-dsn"

# New Relic license key / New Relic lisenziya açarı
NEW_RELIC_LICENSE_KEY="your-new-relic-license-key"

# ===========================================
# BACKUP CONFIGURATION / BACKUP KONFİQURASİYASI
# ===========================================
# Backup schedule / Backup cədvəli
BACKUP_SCHEDULE="0 2 * * *"

# Backup retention days / Backup saxlanma günləri
BACKUP_RETENTION_DAYS="30"

# ===========================================
# PERFORMANCE SETTINGS / PERFORMANS TƏNZİMLƏRİ
# ===========================================
# Cache TTL (in seconds) / Cache TTL (saniyələrlə)
CACHE_TTL="3600"

# Max concurrent requests / Maksimum eyni vaxtda sorğular
MAX_CONCURRENT_REQUESTS="1000"

# Request timeout / Sorğu timeout-u
REQUEST_TIMEOUT="30000"