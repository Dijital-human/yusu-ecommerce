# For critical systems, consider 30000 (30 seconds) / Kritik sistemlər üçün 30000 (30 saniyə) düşünün
ALERT_CHECK_INTERVAL="60000"

# Log Aggregation Configuration (ELK Stack) / Log Aggregation Konfiqurasiyası (ELK Stack)
# Enable Logstash integration (default: true in production) / Logstash inteqrasiyası aktivləşdir (default: production-da true)
LOGSTASH_ENABLED="true"
# Logstash host (production Logstash server address) / Logstash host (production Logstash server ünvanı)
LOGSTASH_HOST="logstash.yourdomain.com"
# Logstash port (default: 5000 for TCP, 5044 for Beats) / Logstash port (default: TCP üçün 5000, Beats üçün 5044)
LOGSTASH_PORT="5000"
# Logstash protocol: tcp, udp, http (default: tcp for production) / Logstash protocol: tcp, udp, http (default: production üçün tcp)
# TCP: Reliable, ordered delivery / TCP: Etibarlı, sıralı çatdırılma
# UDP: Fast, low latency, no delivery guarantee / UDP: Sürətli, aşağı gecikmə, çatdırılma zəmanəti yoxdur
# HTTP: Easy to configure, good for cloud / HTTP: Konfiqurasiya etmək asandır, cloud üçün yaxşıdır
LOGSTASH_PROTOCOL="tcp"
# Logstash service name (default: yusu-ecommerce) / Logstash service adı (default: yusu-ecommerce)
LOGSTASH_SERVICE_NAME="yusu-ecommerce"
# Log batch size for shipping (default: 100, increase for high volume) / Shipping üçün log batch ölçüsü (default: 100, yüksək həcm üçün artırın)
LOGSTASH_BATCH_SIZE="100"
# Log flush interval in milliseconds (default: 5000 = 5 seconds) / Log flush intervalı millisaniyələrlə (default: 5000 = 5 saniyə)
# For high-traffic sites, consider 3000 (3 seconds) / Yüksək trafikli saytlar üçün 3000 (3 saniyə) düşünün
LOGSTASH_FLUSH_INTERVAL="5000"
# Logstash connection timeout in milliseconds (default: 5000) / Logstash bağlantı timeout-u millisaniyələrlə (default: 5000)
LOGSTASH_TIMEOUT="5000"
# Logstash retry attempts (default: 3) / Logstash retry cəhdləri (default: 3)
LOGSTASH_RETRY_ATTEMPTS="3"
# Logstash retry delay in milliseconds (default: 1000) / Logstash retry gecikməsi millisaniyələrlə (default: 1000)
LOGSTASH_RETRY_DELAY="1000"

# Event Bus Configuration / Event Bus Konfiqurasiyası
# Enable event bus (default: true in production) / Event bus aktivləşdir (default: production-da true)
EVENT_BUS_ENABLED="true"
# Maximum event queue size (default: 1000, increase for high-traffic sites) / Maksimum event queue ölçüsü (default: 1000, yüksək trafikli saytlar üçün artırın)
EVENT_BUS_MAX_QUEUE_SIZE="1000"
# Event processing interval in milliseconds (default: 100) / Event processing intervalı millisaniyələrlə (default: 100)
# For high-traffic sites, consider 50ms / Yüksək trafikli saytlar üçün 50ms düşünün
EVENT_BUS_PROCESSING_INTERVAL="100"
# Event handler retry attempts (default: 3) / Event handler retry cəhdləri (default: 3)
EVENT_BUS_RETRY_ATTEMPTS="3"
# Event handler retry delay in milliseconds (default: 1000) / Event handler retry gecikməsi millisaniyələrlə (default: 1000)
EVENT_BUS_RETRY_DELAY="1000"

