# Database Configuration / Veritabanı Konfiqurasiyası
DATABASE_URL="postgresql://username:password@localhost:5432/yusu_ecommerce?connection_limit=10&pool_timeout=10&connect_timeout=5"

# Database Connection Pool Configuration / Veritabanı Connection Pool Konfiqurasiyası
# Connection pool limit (default: 10) / Connection pool limit-i (default: 10)
DATABASE_CONNECTION_LIMIT="10"
# Pool timeout in seconds (default: 10) / Pool timeout saniyələrlə (default: 10)
DATABASE_POOL_TIMEOUT="10"
# Connection timeout in seconds (default: 5) / Connection timeout saniyələrlə (default: 5)
DATABASE_CONNECT_TIMEOUT="5"

# NextAuth.js Configuration / NextAuth.js Konfiqurasiyası
NEXTAUTH_URL="http://localhost:3000"
NEXTAUTH_SECRET="your-secret-key-here"

# OAuth Providers / OAuth Provayderlər
GOOGLE_CLIENT_ID="your-google-client-id"
GOOGLE_CLIENT_SECRET="your-google-client-secret"
FACEBOOK_CLIENT_ID="your-facebook-client-id"
FACEBOOK_CLIENT_SECRET="your-facebook-client-secret"
APPLE_CLIENT_ID="your-apple-client-id"
APPLE_CLIENT_SECRET="your-apple-client-secret"

# Payment Gateway / Ödəniş Gateway-i
STRIPE_PUBLISHABLE_KEY="your-stripe-publishable-key"
STRIPE_SECRET_KEY="your-stripe-secret-key"
STRIPE_WEBHOOK_SECRET="your-stripe-webhook-secret"

# Google Maps API / Google Maps API
NEXT_PUBLIC_GOOGLE_MAPS_API_KEY="your-google-maps-api-key"

# Email Configuration / Email Konfiqurasiyası
EMAIL_SERVER_HOST="smtp.gmail.com"
EMAIL_SERVER_PORT="587"
EMAIL_SERVER_USER="your-email@gmail.com"
EMAIL_SERVER_PASSWORD="your-app-password"
EMAIL_FROM="noreply@azliner.info"

# APM Configuration / APM Konfiqurasiyası
# Enable APM (default: false, true in production) / APM aktivləşdir (default: false, production-da true)
APM_ENABLED="false"
# APM Provider: sentry, newrelic, datadog, custom, none (default: none) / APM Provider: sentry, newrelic, datadog, custom, none (default: none)
APM_PROVIDER="none"
# APM Service Name (default: yusu-ecommerce) / APM Service Adı (default: yusu-ecommerce)
APM_SERVICE_NAME="yusu-ecommerce"
# APM Sample Rate: 0.0-1.0 (default: 1.0 = 100%) / APM Sample Rate: 0.0-1.0 (default: 1.0 = 100%)
APM_SAMPLE_RATE="1.0"

# Alert System Configuration / Alert Sistemi Konfiqurasiyası
# Enable alert system (default: false, true in production) / Alert sistemi aktivləşdir (default: false, production-da true)
ALERT_ENABLED="false"
# Alert check interval in milliseconds (default: 60000 = 60 seconds) / Alert yoxlama intervalı millisaniyələrlə (default: 60000 = 60 saniyə)
ALERT_CHECK_INTERVAL="60000"

# Log Aggregation Configuration (ELK Stack) / Log Aggregation Konfiqurasiyası (ELK Stack)
# Enable Logstash integration (default: false, true in production) / Logstash inteqrasiyası aktivləşdir (default: false, production-da true)
LOGSTASH_ENABLED="false"
# Logstash host (default: localhost) / Logstash host (default: localhost)
LOGSTASH_HOST="localhost"
# Logstash port (default: 5000) / Logstash port (default: 5000)
LOGSTASH_PORT="5000"
# Logstash protocol: tcp, udp, http (default: tcp) / Logstash protocol: tcp, udp, http (default: tcp)
LOGSTASH_PROTOCOL="tcp"
# Logstash service name (default: yusu-ecommerce) / Logstash service adı (default: yusu-ecommerce)
LOGSTASH_SERVICE_NAME="yusu-ecommerce"
# Log batch size for shipping (default: 100) / Shipping üçün log batch ölçüsü (default: 100)
LOGSTASH_BATCH_SIZE="100"
# Log flush interval in milliseconds (default: 5000 = 5 seconds) / Log flush intervalı millisaniyələrlə (default: 5000 = 5 saniyə)
LOGSTASH_FLUSH_INTERVAL="5000"
# Logstash connection timeout in milliseconds (default: 5000) / Logstash bağlantı timeout-u millisaniyələrlə (default: 5000)
LOGSTASH_TIMEOUT="5000"
# Logstash retry attempts (default: 3) / Logstash retry cəhdləri (default: 3)
LOGSTASH_RETRY_ATTEMPTS="3"
# Logstash retry delay in milliseconds (default: 1000) / Logstash retry gecikməsi millisaniyələrlə (default: 1000)
LOGSTASH_RETRY_DELAY="1000"

# Event Bus Configuration / Event Bus Konfiqurasiyası
# Enable event bus (default: true) / Event bus aktivləşdir (default: true)
EVENT_BUS_ENABLED="true"
# Maximum event queue size (default: 1000) / Maksimum event queue ölçüsü (default: 1000)
EVENT_BUS_MAX_QUEUE_SIZE="1000"
# Event processing interval in milliseconds (default: 100) / Event processing intervalı millisaniyələrlə (default: 100)
EVENT_BUS_PROCESSING_INTERVAL="100"
# Event handler retry attempts (default: 3) / Event handler retry cəhdləri (default: 3)
EVENT_BUS_RETRY_ATTEMPTS="3"
# Event handler retry delay in milliseconds (default: 1000) / Event handler retry gecikməsi millisaniyələrlə (default: 1000)
EVENT_BUS_RETRY_DELAY="1000"

# Search Engine Configuration / Axtarış Mühərriki Konfiqurasiyası
# Enable search engine (default: false, true in production) / Axtarış mühərrikini aktivləşdir (default: false, production-da true)
SEARCH_ENGINE_ENABLED="false"
# Meilisearch host (default: http://localhost:7700) / Meilisearch host (default: http://localhost:7700)
MEILISEARCH_HOST="http://localhost:7700"
# Meilisearch API key (optional, for production) / Meilisearch API açarı (istəyə bağlı, production üçün)
MEILISEARCH_API_KEY=""
# Search index batch size (default: 100) / Axtarış indeks batch ölçüsü (default: 100)
SEARCH_INDEX_BATCH_SIZE="100"

# Visual Search Configuration / Vizual Axtarış Konfiqurasiyası
# Enable visual search (default: false, requires computer vision API) / Vizual axtarışı aktivləşdir (default: false, computer vision API tələb edir)
VISUAL_SEARCH_ENABLED="false"
# Visual search provider: google-vision, aws-rekognition, azure-vision, custom (default: none) / Vizual axtarış provider: google-vision, aws-rekognition, azure-vision, custom (default: none)
VISUAL_SEARCH_PROVIDER="none"
# Google Vision API key (if using google-vision) / Google Vision API açarı (google-vision istifadə edərsə)
GOOGLE_VISION_API_KEY=""
# AWS Rekognition region (if using aws-rekognition) / AWS Rekognition region (aws-rekognition istifadə edərsə)
AWS_REKOGNITION_REGION="us-east-1"
# AWS Rekognition access key (if using aws-rekognition) / AWS Rekognition access key (aws-rekognition istifadə edərsə)
AWS_REKOGNITION_ACCESS_KEY=""
# AWS Rekognition secret key (if using aws-rekognition) / AWS Rekognition secret key (aws-rekognition istifadə edərsə)
AWS_REKOGNITION_SECRET_KEY=""
# Azure Vision endpoint (if using azure-vision) / Azure Vision endpoint (azure-vision istifadə edərsə)
AZURE_VISION_ENDPOINT=""
# Azure Vision subscription key (if using azure-vision) / Azure Vision subscription key (azure-vision istifadə edərsə)
AZURE_VISION_SUBSCRIPTION_KEY=""

# ML Model Configuration / ML Model Konfiqurasiyası
# Enable custom ML model for image analysis (default: false, true in production) / Rəsim analizi üçün öz ML modelimizi aktivləşdir (default: false, production-da true)
ML_MODEL_ENABLED="false"
# ML model type: mobilenet, efficientnet (default: mobilenet) / ML model növü: mobilenet, efficientnet (default: mobilenet)
ML_MODEL_TYPE="mobilenet"
# ML model path (optional, for custom models) / ML model yolu (istəyə bağlı, custom modellər üçün)
ML_MODEL_PATH=""
# Embedding dimension (default: 128) / Embedding ölçüsü (default: 128)
ML_EMBEDDING_DIMENSION="128"
# Enable embedding cache (default: true) / Embedding cache aktivləşdir (default: true)
ML_CACHE_ENABLED="true"
