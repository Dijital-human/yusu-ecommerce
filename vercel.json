{
  "version": 2,
  "name": "yusu-ecommerce",
  "builds": [
    {
      "src": "yusu-ecommerce/package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "yusu-ecommerce/$1"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "functions": {
    "yusu-ecommerce/src/app/api/**/*.ts": {
      "runtime": "nodejs18.x"
    }
  }
}
